{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "ValidAudience": "api",
    "ValidIssuer": "<YourIssue>",
    "Secret": "<YourSecret>",
    "TokenValidityInMinutes": 60,
    "RefreshTokenValidityInDays": 7
  },
  "IdentityDatabase": {
    "ConnectionString": "<IdentityDatabase>"
  },
  "BookstoreDatabase": {
    "ConnectionString": "<BookStoreDatabase>"
  },
  "Cors": {
    "AllowedOrigins": "*"
  }
}
